1) RECURSION

    int find(int n , int m , string &x , string &y)
    {
        if(n == 0 || m== 0)
        return 0;
        
        if(x[n-1] == y[m-1])
            return 1+find(n-1,m-1,x,y);
        else
            return max(find(n-1,m,x,y),find(n,m-1,x,y));
    }
    int lcs(int n, int m, string str1, string str2) {
        // your code here
        return find(n,m,str1,str2);
    }
